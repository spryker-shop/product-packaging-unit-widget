  "description": "ProductPackagingUnitWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/money-widget": "^1.0.0",
    "spryker/kernel": "^3.16.0",
    "spryker/product-measurement-unit-storage": "^1.1.0",
    "spryker/product-packaging-unit-storage": "^1.0.0",
